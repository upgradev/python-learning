            "source": [
                "# Given a three-digit number. Find the sum of its digits.\n"
            ],
            "metadata": {}
        },
        {
            "cell_type": "code",
            "execution_count": 24,
            "source": [
                "number = int(input(\"Enter the integer number: \"))\n",
                "n1 = number // 100\n",
                "n2 = number % 100\n",
                "n3 = n2 // 10\n",
                "n4 = n2 % 10\n",
                "print(n1 + n3 + n4)"
            ],
